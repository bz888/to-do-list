{
  "name": "to-do-list",
  "version": "1.0.0",
  "description": "todolist",
  "main": "server/public/index.js",
  "scripts": {
    "dev": "run-p client server",
    "client": "webpack --watch",
    "server": "nodemon server",
    "start": "ts-node server",
    "test": "jest --watchAll",
    "lint": "eslint --ext .ts,.tsx .",
    "webpack": "webpack --config ./webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bz888/to-do-list.git"
  },
  "author": "bz888/saritodeni",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bz888/to-do-list/issues"
  },
  "homepage": "https://github.com/bz888/to-do-list#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "jest"
    ],
    "extends": [
      "standard",
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:jest/recommended",
      "plugin:jest/style"
    ],
    "rules": {
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": [
        "error"
      ],
      "semi": [
        "error",
        "never"
      ],
      "quotes": [
        2,
        "single"
      ],
      "eol-last": [
        "error",
        "always"
      ],
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 1,
          "maxEOF": 0,
          "maxBOF": 0
        }
      ],
      "object-curly-spacing": [
        2,
        "always"
      ],
      "react/prop-types": 0
    }
  },
  "dependencies": {
    "@mantine/core": "^4.1.0",
    "@mantine/dates": "^4.1.0",
    "@mantine/form": "^4.1.0",
    "@mantine/hooks": "^4.1.0",
    "@mantine/notifications": "^4.1.0",
    "dayjs": "^1.11.0",
    "express": "^4.17.1",
    "ts-node": "^10.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/register": "^7.13.14",
    "@reduxjs/toolkit": "^1.6.2",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "fork-ts-checker-webpack-plugin": "^6.4.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.34.0",
    "webpack-cli": "^4.6.0"
  }
}
