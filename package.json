{
  "name": "to-do-list",
  "version": "1.0.0",
  "description": "todolist",
  "main": "server/public/index.js",
  "scripts": {
    "build": "webpack --config ./client/webpack.config.js",
    "dev:client": "npm run build -- --watch",
    "dev:server": "nodemon server",
    "dev": "run-p dev:client dev:server",
    "lint": "eslint --ext .js,.jsx .",
    "start": "run-p webpack server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bz888/to-do-list.git"
  },
  "author": "bz888/saritodeni",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bz888/to-do-list/issues"
  },
  "homepage": "https://github.com/bz888/to-do-list#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-regenerator",
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": "eda",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.17.8",
    "core-js": "^3.21.1",
    "express": "^4.17.1",
    "jest": "^26.6.3",

    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/plugin-transform-regenerator": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "7.9.0",
    "eslint-config-eda": "0.1.4",
    "jest": "^26.6.3",
    "jsdom": "^16.4.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.2.2",
    "regenerator-runtime": "^0.13.7",
    "superagent": "^6.1.0",
    "supertest": "^6.1.3",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0"
  }
}
